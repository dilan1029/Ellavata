$(eval if(`$(urlfetch json http://twitch.center/customapi/addquote$(query))`.startsWith("Successfully added entry ")){v=`$(urlfetch json http://twitch.center/customapi/quote/list?token=$(urlfetch https://twitch.center/customapi/quote?token=e1346cc9&data=$(querystring))&no_id=1)`.split(",");c=0;for(i=0;i<v.length;i++){m=v[i].match(/^(\w+)\|(\d+)$/);if(m&&m[1].toLowerCase()==`$(user)`.toLowerCase()){c++}}`$(user) says hey! (${c} times)`}else if(`$(query)`=="$TOORECENT$"){`You used !hey less than 6 hours ago, $(user)`}else{"Usa !pan"})
